{
  "description": "Render LaTeX math parts in your markdown files when viewing them on GitHub",
  "manifest_version": 3,
  "name": "LatexMathifyGitHub",
  "version": "1.2",
  "homepage_url": "https://github.com/AnonymerNiklasistanonym/LatexMathifyGitHub",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; font-src 'github.com'",
    "sandbox": "sandbox allow-scripts; object-src 'self'"

  },
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/*"
      ],
      "css": [
        "katex/katex.min.css"
      ],
      "js": [
        "katex/katex.min.js",
        "enableMarkdownLatexMath.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "open_in_tab": false
  }
}
